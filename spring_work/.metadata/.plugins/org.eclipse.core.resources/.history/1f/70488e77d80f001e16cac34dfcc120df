package com.example.demo.model;

import java.util.ArrayList;
import java.util.Iterator;
import java.util.TreeSet;

public class StudList {

	ArrayList<Stud> stList;
	TreeSet<Stud> stSet;
	public void rankCalc() {
		
		Iterator<Stud> it = stList.iterator();
		while(it.hasNext()) {
			Stud st = it.next();
			System.out.println(st.name);
			if(st.name==null ||  st.name.equals("")) {
				it.remove();
			}
		}
		/*for (Stud st : stList) {
			System.out.println(st.getName());
//			if(st.name==null ||  st.name.equals("")) {
//				stList.remove(st);
//			}
		}*/
		for (Stud st : stList) {
			st.setRank(stList);
		}
		
		stSet = new TreeSet(stList);
	}
	public TreeSet<Stud> getStSet() {
		return stSet;
	}
	public void setStList(ArrayList<Stud> stList) {
		this.stList = stList;
	}
	public ArrayList<Stud> getStList() {
		return stList;
	}
	
}
