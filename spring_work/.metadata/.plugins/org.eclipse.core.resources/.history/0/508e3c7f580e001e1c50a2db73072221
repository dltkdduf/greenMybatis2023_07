package aop_main;

import org.springframework.context.annotation.AnnotationConfigApplicationContext;
import org.springframework.context.support.AbstractApplicationContext;

import aop_anno.model.HandPhone;
import aop_device.AopConfig2;

public class Config2Main {

	public static void main(String[] args) {
		
		AbstractApplicationContext context =
				new AnnotationConfigApplicationContext(AopConfig2.class);
		
		HandPhone handPhone = context.getBean("handPhone", HandPhone.class);
		Object oo;
		System.out.println("-----------------");
		handPhone.call();
		System.out.println("-----------------");
		oo = handPhone.photo("ì‚°");
		System.out.println("oo:"+oo);
		System.out.println("-----------------");
		oo = handPhone.photo2();
		System.out.println("oo:"+oo);
		System.out.println("-----------------");
		oo = handPhone.gameMarble();
		System.out.println("oo:"+oo);
		System.out.println("-----------------");
		oo = handPhone.nummm(20);
		System.out.println("oo:"+oo);
		System.out.println("-----------------");
		//oo = handPhone.nummm(0);
		//System.out.println("oo:"+oo);
		System.out.println("-----------------");
		
		context.close();

	}
}
