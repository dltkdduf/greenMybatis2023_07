package com.example.demo.model;

public class Gimbab {
	String name;
	int price, cnt;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	public int getCnt() {
		return cnt;
	}
	public void setCnt(int cnt) {
		this.cnt = cnt;
	}
	
	public int getTotal() {
		return price * cnt;
	}
	
	@Override
	public String toString() {
		return "Gimbab [name=" + name + ", price=" + price + ", cnt=" + cnt + "]";
	}
	
	
}
