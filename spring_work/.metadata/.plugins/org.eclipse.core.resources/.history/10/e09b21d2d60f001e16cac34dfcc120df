package com.example.demo.model;

import java.util.ArrayList;


public class Stud implements Comparable<Stud>{

	String name;
	ArrayList<String> jum;
	int rank;
	
	public int tot() {
		int tot = 0;
		for (String i : jum) {
		//	tot+= Integer.parseInt(i);
		}
		return tot;
	}
	public int avg() {
		return tot()/3;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	
	
	public ArrayList<String> getJum() {
		return jum;
	}
	public void setJum(ArrayList<String> jum) {
		
		
		
		this.jum = jum;
		
	}
	
	public int getRank() {
		return rank;
	}
	public void setRank(ArrayList<Stud> studs) {
		rank = 1;
		for (Stud st : studs) {
			if(avg() < st.avg()) {
				rank++;
			}
		}
	}
	
	@Override
	public String toString() {
		return "Stud [name=" + name + ", jum=" + jum + ", rank=" + rank + "]";
	}
	@Override
	public int compareTo(Stud o) {
		int res = o.avg()-avg();
		if(res==0) {
			res = name.compareTo(o.name);
		}
		return res;
	}
	
}
